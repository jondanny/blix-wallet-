{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2020",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@api/*": [
        "apps/api/src/*"
      ],
      "@test/*": [
        "apps/api/test/*"
      ],
      "@producer/*": [
        "apps/producer/src/*"
      ],
      "@consumer/*": [
        "apps/consumer/src/*"
      ],
      "@app/ticket-provider-encryption-key": [
        "libs/ticket-provider-encryption-key/src"
      ],
      "@app/ticket-provider-encryption-key/*": [
        "libs/ticket-provider-encryption-key/src/*"
      ],
      "@app/database": [
        "libs/database/src"
      ],
      "@app/database/*": [
        "libs/database/src/*"
      ],
      "@app/env": [
        "libs/env/src"
      ],
      "@app/env/*": [
        "libs/env/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/ticket": [
        "libs/ticket/src"
      ],
      "@app/ticket/*": [
        "libs/ticket/src/*"
      ],
      "@app/event": [
        "libs/event/src"
      ],
      "@app/event/*": [
        "libs/event/src/*"
      ],
      "@app/ticket-provider": [
        "libs/ticket-provider/src"
      ],
      "@app/ticket-provider/*": [
        "libs/ticket-provider/src/*"
      ],
      "@app/ticket-transfer": [
        "libs/ticket-transfer/src"
      ],
      "@app/ticket-transfer/*": [
        "libs/ticket-transfer/src/*"
      ],
      "@app/ticket-type": [
        "libs/ticket-type/src"
      ],
      "@app/ticket-type/*": [
        "libs/ticket-type/src/*"
      ],
      "@app/user": [
        "libs/user/src"
      ],
      "@app/user/*": [
        "libs/user/src/*"
      ],
      "@app/outbox": [
        "libs/outbox/src"
      ],
      "@app/outbox/*": [
        "libs/outbox/src/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/sentry": [
        "libs/sentry/src"
      ],
      "@app/sentry/*": [
        "libs/sentry/src/*"
      ]
    }
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  }
}