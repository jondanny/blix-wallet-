{
  "name": "validate-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "api:start": "nest start api",
    "api:start:dev": "nest start api --watch",
    "api:start:debug": "nest start api --debug --watch",
    "api:start:prod": "node dist/api/main",
    "web:start": "nest start web",
    "web:start:dev": "nest start web --watch",
    "web:start:debug": "nest start web --debug --watch",
    "web:start:prod": "node dist/web/main",
    "producer:start": "nest start producer",
    "producer:start:dev": "nest start producer --watch",
    "producer:start:debug": "nest start producer --debug --watch",
    "producer:start:prod": "node dist/producer/main",
    "consumer:start": "nest start consumer",
    "consumer:start:dev": "nest start consumer --watch",
    "consumer:start:debug": "nest start consumer --debug --watch",
    "consumer:start:prod": "node dist/consumer/main",
    "test": "jest --config ./apps/api/test/jest.json --maxWorkers=1 --forceExit --verbose",
    "test:ci": "jest --config ./apps/api/test/jest.json --ci --reporters=default --reporters=jest-junit --maxWorkers=1 --logHeapUsage --colors --forceExit",
    "test:watch": "jest --config ./apps/api/test/jest.json --watch",
    "test:cov": "jest --config ./apps/api/test/jest.json --coverage",
    "test:debug": "node --config ./apps/api/test/jest.json --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migration:run": "NODE_ENV=development npx typeorm-ts-node-commonjs migration:run --dataSource ./libs/database/src/config/datasource.ts",
    "migration:run:ci": "NODE_ENV=test npx typeorm-ts-node-commonjs migration:run --dataSource ./libs/database/src/config/datasource.ts",
    "migration:revert": "dotenv -e .env.development ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert",
    "migration:generate": "dotenv -e .env.development ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate --",
    "migration:create": "dotenv -e .env.development ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:create --",
    "repl": "npm run start -- --entryFile repl"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/microservices": "^9.1.4",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@sentry/integrations": "^7.13.0",
    "@sentry/node": "^7.13.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "hashids": "^2.2.10",
    "ioredis": "^5.2.4",
    "jest-junit": "^14.0.1",
    "kafkajs": "^2.2.0",
    "lodash": "^4.17.21",
    "luxon": "^3.1.0",
    "mysql2": "^2.3.3",
    "nanoid": "^2.1.11",
    "npm": "^8.19.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-unique-token": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stripe": "^11.7.0",
    "typeorm": "^0.3.10",
    "typeorm-cursor-pagination": "^0.9.1",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@sentry/types": "^7.13.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.189",
    "@types/luxon": "^3.0.2",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 15000,
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/ticket-provider-encryption-key(|/.*)$": "<rootDir>/libs/ticket-provider-encryption-key/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/env(|/.*)$": "<rootDir>/libs/env/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/ticket(|/.*)$": "<rootDir>/libs/ticket/src/$1",
      "^@app/event(|/.*)$": "<rootDir>/libs/event/src/$1",
      "^@app/ticket-provider(|/.*)$": "<rootDir>/libs/ticket-provider/src/$1",
      "^@app/ticket-transfer(|/.*)$": "<rootDir>/libs/ticket-transfer/src/$1",
      "^@app/ticket-type(|/.*)$": "<rootDir>/libs/ticket-type/src/$1",
      "^@app/user(|/.*)$": "<rootDir>/libs/user/src/$1",
      "^@app/outbox(|/.*)$": "<rootDir>/libs/outbox/src/$1",
      "^@app/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@app/sentry(|/.*)$": "<rootDir>/libs/sentry/src/$1",
      "^@app/inbox(|/.*)$": "<rootDir>/libs/inbox/src/$1",
      "^@app/listing(|/.*)$": "<rootDir>/libs/listing/src/$1",
      "^@app/message(|/.*)$": "<rootDir>/libs/message/src/$1",
      "^@app/order(|/.*)$": "<rootDir>/libs/order/src/$1",
      "^@app/redeem(|/.*)$": "<rootDir>/libs/redeem/src/$1"
    }
  }
}
